@model IEnumerable<JoseMathew_200298627_FinalExam.Models.Artist>

@{
    ViewBag.Title = "Index";
}

<h2>Browse Artists</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
      
    </tr>
}

</table>
